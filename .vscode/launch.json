{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-node",
        "request": "launch",
        "name": "Launch via NPM",
        "runtimeExecutable": "npm",
        "windows": {
          "runtimeExecutable": "npm"
        },
        "runtimeArgs": [
          "run-script",
          "start:debug"
        ],
        "port": 5858,
        "console": "integratedTerminal",
        "skipFiles":[
            "<node_internals>/**"
        ] 
      }
    ]
  }